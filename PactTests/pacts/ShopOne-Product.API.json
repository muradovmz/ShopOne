{
  "consumer": {
    "name": "ShopOne"
  },
  "interactions": [
    {
      "description": "a request to retrieve all products",
      "request": {
        "method": "GET",
        "path": "/Product/list"
      },
      "response": {
        "body": [
          {
            "id": 1,
            "name": "ragaca1",
            "price": 10
          }
        ],
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            }
          },
          "header": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "ffi": "0.4.0",
      "models": "1.0.4"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "Product.API"
  }
}